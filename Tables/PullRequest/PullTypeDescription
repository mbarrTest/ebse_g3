        VAR Numerator =
            SUMX(
                ALL('PullRequestEncoded'),
                ('PullRequestEncoded'[classification_design_debt] - XMean) *
                ('PullRequestEncoded'[pull_type_description] - YMean)
            )
        VAR Denominator =
            SQRT(
                SUMX(
